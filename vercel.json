{
  "version": 2,
  "buildCommand": "cd frontend && npm run vercel-build",
  "outputDirectory": "frontend/build",
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/main.py" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "functions": {
    "api/main.py": {
      "runtime": "python@3.10"
    }
  }
}